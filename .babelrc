{
"presets": [["@babel/preset-env", {
    "targets": {
      "ie": "8",
      "edge": "17",
      "firefox": "60",
      "chrome": "67",
      "safari": "11.1"
    },
    "useBuiltIns": "usage"
  }]],
  "plugins": [
    ["@babel/plugin-proposal-private-methods", { "loose": true }],
    ["@babel/plugin-transform-runtime", {
      "corejs": 2,
      "helpers": true,
      "regenerator": true,
      "useESModules": false
    }],
    ["@babel/plugin-proposal-class-properties", { "loose": true }]
  ]
}
